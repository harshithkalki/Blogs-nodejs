<!DOCTYPE html>
<html lang="en">
<%-include('./partials/head.ejs')%>
<body>
    <%-include('./partials/nav.ejs')%>
    <main>
        <div class="blogdata content">
    <% if(blogs.length>0){ %>
        <% blogs.forEach(blog=>{%>
            <a href="/blog/<%=blog._id %>">
            <h1><%= blog.title%></h1>
            <p><%= blog.snippet%></p>
        </a>
        <%})%>
    <% } else {%>
        <h1>NO BLOGS</h1>
    <%}%>
</div>
</main>
    <%-include('./partials/footer.ejs')%>
</body>
</html>